class Solution {
    public boolean isPalindrome(int x) {
                if (x<0){
            return false;

        }
        int y = x;
        int rev = 0;
        int digit;
        while (x!=0){
            digit = x %10;
            rev = rev*10+digit;
            x = x/10;
        }

        if(y==rev){
            return true;
        }
        else{
            return false;

        }
        
    }
    
}
